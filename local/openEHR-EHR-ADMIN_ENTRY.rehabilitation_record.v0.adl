						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {    -- Rehabilitation point of pathway
						value matches {
							DV_CODED_TEXT matches {*}
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {    -- Rehabilitation intent
						value matches {
							DV_CODED_TEXT matches {*}
						}
					}
				}
			}
		}
	}

